#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/User/Desktop/Lowvield Academy/Application/LowveldAcademyApp/node_modules/.pnpm/vitest@3.2.4_@types+node@24_6c277f288b080449e1db66cb43665907/node_modules/vitest/node_modules:/mnt/c/Users/User/Desktop/Lowvield Academy/Application/LowveldAcademyApp/node_modules/.pnpm/vitest@3.2.4_@types+node@24_6c277f288b080449e1db66cb43665907/node_modules:/mnt/c/Users/User/Desktop/Lowvield Academy/Application/LowveldAcademyApp/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/User/Desktop/Lowvield Academy/Application/LowveldAcademyApp/node_modules/.pnpm/vitest@3.2.4_@types+node@24_6c277f288b080449e1db66cb43665907/node_modules/vitest/node_modules:/mnt/c/Users/User/Desktop/Lowvield Academy/Application/LowveldAcademyApp/node_modules/.pnpm/vitest@3.2.4_@types+node@24_6c277f288b080449e1db66cb43665907/node_modules:/mnt/c/Users/User/Desktop/Lowvield Academy/Application/LowveldAcademyApp/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi
